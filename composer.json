{
    "name": "omatech/mage-orders",
    "description": "Laravel Orders special package for mage",
    "keywords": [
        "omatech",
        "mage-orders"
    ],
    "homepage": "https://github.com/omatech/mage-orders",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "HÃ©ctor Arnau",
            "email": "harnau@omatech.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Omatech\\MageOrders\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omatech\\MageOrders\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Omatech\\MageOrders\\MageOrdersServiceProvider"
            ]
        }
    }
}
